{
  "roleName": "Lambda-Honeycode-Getter",
  "policies": [
    {
      "name": "AWSLambdaBasicExecutionRole",
      "id": "ANPAJNCQGXC42545SKXIK",
      "type": "managed",
      "arn": "arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole",
      "document": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Effect": "Allow",
            "Action": [
              "logs:CreateLogGroup",
              "logs:CreateLogStream",
              "logs:PutLogEvents"
            ],
            "Resource": "*"
          }
        ]
      }
    },
    {
      "name": "AmazonHoneycodeReadOnlyAccess",
      "id": "ANPAZKAPJZG4CRFGMHZ3B",
      "type": "managed",
      "arn": "arn:aws:iam::aws:policy/AmazonHoneycodeReadOnlyAccess",
      "document": {
        "Version": "2012-10-17",
        "Statement": [
          {
            "Action": [
              "honeycode:List*",
              "honeycode:Get*",
              "honeycode:Describe*",
              "honeycode:Query*"
            ],
            "Resource": "*",
            "Effect": "Allow"
          }
        ]
      }
    }
  ]
}